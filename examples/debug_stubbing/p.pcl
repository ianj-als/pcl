import parallel_component as para
import sequential_component as seq

component p
  inputs a, b
  outputs (c, d), (para_e, para_f, para_g, para_h)
  declare
    para := new para
    seq := new seq
  as
    wire a -> c, b -> d &&&

    (split >>>

     (wire (a -> a, b -> b), (a -> c, b -> d) >>>

     (para >>>

     merge top[e] -> para_e, top[f] -> para_f,
           bottom[g] -> para_g, bottom[h] -> para_h)))
